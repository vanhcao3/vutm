    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "postcss": "^8.4.23",
    "prettier": "^2.3.0",
